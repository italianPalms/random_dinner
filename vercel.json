{
    "version": 2, 
    "builds": [
        {
            "src": "Backend/server.js", 
            "use": "@vercel/node"
        },
        {
            "src": "dist/**", 
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/Backend/server.js"
        },
        {
            "src": "/(.*)", 
            "dest": "/dist/$1"
        }
    ]
}